<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:smartdost="http://schemas.android.com/apk/res/com.samsung.ssc"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:background="@color/white"
    android:orientation="vertical" >

  <com.samsung.ssc.CustomUI.SSCTextView
      android:id="@+id/beatDate"
      android:layout_width="fill_parent"
      android:layout_height="wrap_content"
      android:layout_marginLeft="10dp"
      android:layout_marginRight="10dp"
      android:layout_marginTop="4dp"
      android:gravity="right|center_vertical|center_horizontal"
      android:singleLine="false"
       android:textColor="#8F8080"
      android:textSize="@dimen/text_size_small_seventeen"
      android:textStyle="bold"
      smartdost:fontType="neuton_extralight" />
    
    <com.samsung.ssc.CustomUI.SSCTextView
        android:id="@+id/storeName"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        android:singleLine="false"
        android:textColor="@color/text_color"
        android:textSize="@dimen/text_size_smaller"
        smartdost:fontType="roboto_medium"
        android:text=""
        />
    

  
</LinearLayout><!-- From: file:/F:/SC_TFS/SalesCatalyst%20Android/Dev/SC_STUDIO/app/src/main/res/layout/beat_details_list_items.xml -->